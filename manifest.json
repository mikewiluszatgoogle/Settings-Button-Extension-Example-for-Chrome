{
"update_url": "https://clients2.google.com/service/update2/crx",
	"name"					: "Settings Button Example"

, 	"version"				: "2.1"

,	"manifest_version"		: 2
,	"homepage_url"			: "https://github.com/mikewiluszatgoogle/Settings-Button-Extension-Example-for-Chrome"
,	"description"			: "Example shortcut to Chrome settings page"

,	"icons"					:
		{  "16"				: "icon.png"
		,  "48"				: "icon-48.png"
		, "128"				: "icon-128.png"
		}
,	"browser_action"		:
		{ "default_icon"	: "icon.png"
		, "default_title" : "Settings"
		}

,	"incognito"				: "spanning"
,	"background"			: 
		{ "scripts": ["background.js"]
		, "persistent" : false
		}
	
}

